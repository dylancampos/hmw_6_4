				} else if (friends[i].get_bday().substr(0, 2) != "05") {
					cout << "No students were born in May" << endl;
				}
			}
		}

		else if (month == "June")
		{
			cout << "\nList of students who were born in June: " << endl;
			for (int i = 0; i < friends.size(); i++)
			{
				if (friends[i].get_bday().substr(0, 2) == "06")
