		else if (month == "September")
		{
			cout << "\nList of students who were born in September: " << endl;
			for (int i = 0; i < friends.size(); i++)
			{
				if (friends[i].get_bday().substr(0, 2) == "09")
				{
				cout << friends[i].get_name() << " " << friends[i].get_bday() << endl;
				} else if (friends[i].get_bday().substr(0, 2) != "01" && "02" && "03" && "04" && "05" && "06"&& "07" && "08" && "09"&&"10" && "11" && "12") {
					cout << "No students were born in September" << endl;
				}
			}	
		}
		else if (month == "October")
		{
			cout << "\nList of students who were born in October: " << endl;
