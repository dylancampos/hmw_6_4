 if (month == "January")
		{
			cout << "\nList of students who were born in January: " << endl;
			for (int i = 0; i < friends.size(); i++)
			{
				if (friends[i].get_bday().substr(0,2) == "01")
				{
					cout << friends[i].get_name() << " " << friends[i].get_bday() << endl;
				} else if(friends[i].get_bday().substr(0, 2) != "01") {
					cout << "No students were born in January" << endl;
					break;
				}
			}
		}
		else if (month == "February")
		{
			cout << "\nList of students who were born in February: " << endl;
			for (int i = 0; i < friends.size(); i++)
			{
				if (friends[i].get_bday().substr(0, 2) == "02")
				{
					cout << friends[i].get_name() << " " << friends[i].get_bday() << endl;
				} else if (friends[i].get_bday().substr(0,2) != "02") {
					cout << "No students were born in February" << endl;
					break;
				}
			}
		}
		else if (month == "March")
		{
			cout << "\nList of students who were born in March: " << endl;
