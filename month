					cout << friends[i].get_name() << " " << friends[i].get_bday() << endl;
				} else if (friends[i].get_bday().substr(0, 2) != "10") {
		cout << "No students were born in October" << endl;
			}
			}
		}
		else if (month == "November")
		{
		cout << "\nList of students who were born in November: " << endl;
		for (int i = 0; i < friends.size(); i++)
		{
			if (friends[i].get_bday().substr(0, 2) == "11")
