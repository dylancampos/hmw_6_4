		else if (month == "December")
		{
		cout << "\nList of students who were born in December: " << endl;
		for (int i = 0; i < friends.size(); i++)
		{
			if (friends[i].get_bday().substr(0, 2) == "12")
			{
				cout << friends[i].get_name() << " " << friends[i].get_bday() << endl;
			} else if (friends[i].get_bday().substr(0, 2) != "12") {
				cout << "No students were born in December" << endl;
			}
		}
		}

		if (month != "January" || "February" || "March"|| "April"|| "May"|| "June" || "July" || "August" || "September" || "October"|| "November" || "December" )
		{
		cout << "There is no such month. Try Again." << endl;
		}
